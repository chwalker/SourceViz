<h1>Listing daily_histograms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tweet count</th>
      <th>Since</th>
      <th>Histogram</th>
      <th>Metric</th>
      <th>Stream</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_histograms.each do |daily_histogram| %>
      <tr>
        <td><%= daily_histogram.name %></td>
        <td><%= daily_histogram.tweet_count %></td>
        <td><%= daily_histogram.since_id %></td>
        <td><%= daily_histogram.histogram %></td>
        <td><%= daily_histogram.metric %></td>
        <td><%= daily_histogram.stream %></td>
        <td><%= link_to 'Show', daily_histogram %></td>
        <td><%= link_to 'Edit', edit_daily_histogram_path(daily_histogram) %></td>
        <td><%= link_to 'Destroy', daily_histogram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily histogram', new_daily_histogram_path %>
