<h1>Listing hourly_histograms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tweet count</th>
      <th>Since</th>
      <th>Histogram</th>
      <th>Metric</th>
      <th>Stream</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hourly_histograms.each do |hourly_histogram| %>
      <tr>
        <td><%= hourly_histogram.name %></td>
        <td><%= hourly_histogram.tweet_count %></td>
        <td><%= hourly_histogram.since_id %></td>
        <td><%= hourly_histogram.histogram %></td>
        <td><%= hourly_histogram.metric %></td>
        <td><%= hourly_histogram.stream %></td>
        <td><%= link_to 'Show', hourly_histogram %></td>
        <td><%= link_to 'Edit', edit_hourly_histogram_path(hourly_histogram) %></td>
        <td><%= link_to 'Destroy', hourly_histogram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hourly histogram', new_hourly_histogram_path %>
